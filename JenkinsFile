pipeline {
    agent any

    environment {
    		DOCKERHUB_CREDENTIALS=credentials('dock')
    	}

    stages {
        stage('Récupération du code') {
            steps {
                checkout scm
            }
        }

        stage('remove old release') {
            steps {               
                sh 'mvn clean'
            }
        }

        stage('compiling') {
            steps {               
                sh 'mvn compile'
            }
        }

       
stage(' NETWORK Scan ') {
            steps {
        sh 'nmap  192.168.33.10 > /home/vagrant/scan_results.txt1  '
    }
       }
               

        

}
}
